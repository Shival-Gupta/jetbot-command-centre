{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 🛠️ Other Changes",
      "labels": ["other"]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": ["dependencies"]
    }
  ],
  "template": "${{CHANGELOG}}\n\n## 📦 Docker Images\n- `ghcr.io/shival-gupta/jetbot-command-centre:${{TAG}}`\n- `docker.io/shivalgupta5545/jetbot-command-centre:${{TAG}}`",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365
}